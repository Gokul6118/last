{
  "version": 2,
  "functions": {
    "api/index.ts": {
      "runtime": "@vercel/node@2.0.0"
    }
  },
  "rewrites": [
    { "source": "/todos", "destination": "/api/index" },
    { "source": "/todos/:id", "destination": "/api/index" },
    { "source": "/api/:path*", "destination": "/api/index" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
